          "options": {
            "cypressConfig": "apps/zac-ui-e2e/cypress.json",
            "tsConfig": "apps/zac-ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "zac-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "zac-ui:serve:production"
            }
          }
        }
      },
      "tags": [],
      "implicitDependencies": [
        "zac-ui"
      ]
    }
